@model List<Cars.Core.Domain.Car>

<h1>Cars</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Brand</th>
            <th>Model</th>
            <th>Year</th>
            <th>Horsepower</th>
            <th>Created At</th>
            <th>Updated At</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var car in Model)
        {
            <tr>
                <td>@car.Id</td>
                <td>@car.Brand</td>
                <td>@car.Model</td>
                <td>@car.Year</td>
                <td>@car.Horsepower</td>
                <td>@car.CreatedAt</td>
                <td>@car.UpdatedAt</td>
                <td>
                    <a href="#">Edit</a>
                </td>
            </tr>
        }
    </tbody>

</table>